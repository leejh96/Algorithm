#include <iostream>

using namespace std;

int main()
{
    int x;
    cin >>x;

    for(int i = 0; i<x; i++){
        int a, b, c, d;
        int h, m;
        cin >> a >> b >> c >> d;

        h = a+c;
        m = b+d;
        if(h>12){
            h = h -12;
        }
        if(m > 60){
            h++;
            m = m - 60;
        }
        cout << "#"<< i+1 << " " << h << " " << m <<endl;
    }

    return 0;
}
