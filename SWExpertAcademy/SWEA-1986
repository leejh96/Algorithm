#include <iostream>

using namespace std;

int main()
{
    int x;
    cin >> x;

    for(int i = 0; i < x; i++){
        int sum = 0;
        int y;
        cin >> y;
        for(int j = 1; j<=y; j++){
            if(j % 2 == 0){
                sum = sum - j;
            }
            else{
                sum = sum + j;
            }
        }
        cout << "#"<<i+1<<" "<<sum<<endl;
    }
    return 0;
}
